@{
}
@model List<Model_View_Control.Models.Entity.Student>
<h1>Students Data</h1>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Email</th>
            <th>Phone</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var  stu in Model)
        {
            <tr>
                <td>@stu.Id</td>
                <td>@stu.Name</td>
                <td>@stu.email</td>
                <td>@stu.Phone</td>
                <td>
                    <a class="btn-primary" asp-controller="Students" asp-action="Edit" asp-route-id="@stu.Id">Edit</a>
                </td>
            </tr>

        }
    </tbody>
</table>